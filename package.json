{
  "name": "expenseit-api",
  "version": "1.0.0",
  "description": "Backend API for Expense tracker, ExpenseIt",
  "keywords": [
    "ExpenseIt",
    "Budgeting",
    "Nodejs"
  ],
  "license": "ISC",
  "author": "Leonard Atorough",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "ai:list": "node tools/ai-runner.js list",
    "ai:run": "node tools/ai-runner.js",
    "build": "tsc",
    "dev": "tsx index.ts",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@tsconfig/node-ts": "^23.6.2",
    "@tsconfig/node24": "^24.0.3",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  }
}
